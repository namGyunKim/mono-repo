{
  "name": "user-api",
  "projectType": "application",
  "sourceRoot": "apps/user-api/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew :apps:user-api:build",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew :apps:user-api:bootRun",
        "cwd": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew :apps:user-api:test",
        "cwd": "."
      }
    }
  }
}
